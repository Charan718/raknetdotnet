<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="SWIG Rules"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="SWIG_csharp"
			DisplayName="SWIG C#"
			CommandLine="..\swigwin\swig.exe -namespace RakNetDotNet -dllimport $(ProjectName) -c++ -csharp -o $(InputName)_wrap.cxx -outdir ..\RakNetDotNet &quot;$(InputPath)&quot;&#x0D;&#x0A;..\ruby\bin\ruby proxy-modifier.rb ..\RakNetDotNet\RakNetPINVOKE.cs"
			Outputs="$(InputName)_wrap.cxx;$(InputName)_wrap.h"
			FileExtensions="*.i"
			ExecutionDescription="Generating SWIG wrapper..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="ERB"
			DisplayName="SWIG ERB Preprocessor"
			CommandLine="..\ruby\bin\ruby swig-renderer.rb $(InputFileName)"
			Outputs="$(InputName).i"
			AdditionalDependencies="swig-renderer.rb;type-mapper.rb"
			FileExtensions="*.ri"
			ExecutionDescription="Generating SWIG interface..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>

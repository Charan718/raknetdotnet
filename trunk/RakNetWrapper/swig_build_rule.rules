<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="SWIG Rules"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="SWIG_csharp"
			DisplayName="SWIG C#"
			CommandLine="..\swigwin\swig.exe -namespace RakNetDotNet -dllimport $(ProjectName) -c++ -csharp -o $(InputName)_wrap.cxx -outdir ..\RakNetDotNet &quot;$(InputPath)&quot;"
			Outputs="$(InputName)_wrap.cxx"
			FileExtensions="*.i"
			ExecutionDescription="Generating SWIG wrapper..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="ERB"
			DisplayName="SWIG ERB Preprocessor"
			CommandLine="..\ruby\bin\erb -P $(InputFileName) &gt; $(InputName).i"
			Outputs="$(InputName).i"
			FileExtensions="*.ri"
			ExecutionDescription="Generating SWIG interface..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
